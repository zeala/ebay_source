

package ebayApp.services.dto
{
	/**
	 * Provides constants for the ErrorClassificationCodeType simple type.
	 * 
	 * <p>This class identifies the code types that eBay could use to identify 
	 * the error classifications.</p>
	 * 
	 * @langversion ActionScript 3.0
	 * @playerversion Flash 9
	 * @tiptext
	 * 
	 * @author Charles Bihis, Adobe Systems Incorporated
	 * 
	 * @see http://developer.ebay.com/DevZone/SOAP/docs/WSDL/xsd/1/simpletype/ErrorClassificationCodeType.htm
	 */	
	public final class ErrorClassification
	{
		public static const REQUEST_ERROR:String = "RequestError";
		public static const SYSTEM_ERROR:String = "SystemError";
		public static const CUSTOM_CODE:String = "CustomCode";
	}  // class declaration
}  // package