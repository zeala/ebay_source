<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 width="200" height="200"
		 scaleGridLeft="20" scaleGridRight="100" 
		 scaleGridTop="20" scaleGridBottom="100"
		 >
	<s:Rect width="{width}" height="{height}" radiusX="3" radiusY="3">
		<s:stroke>
			<s:SolidColorStroke color="0x676767"/>
		</s:stroke>
		<s:fill>
			<s:SolidColor alpha=".2" color="0xffffff"/>
		</s:fill>
	</s:Rect>
	
	<s:Rect left="5" right="5" top="5" bottom="5">
		<s:fill>
			<s:SolidColor color="0xffffff"/>
		</s:fill>
	</s:Rect>
</s:Group>
