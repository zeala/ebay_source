<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx"
		minWidth="30" minHeight="12">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.HScrollBar")]
	</fx:Metadata>
	<fx:Script>
		<![CDATA[
			
		]]>
	</fx:Script>
	
	<!-- states -->
	<s:states>
		<s:State name="inactive" />
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<!---  The default skin class is HScrollBarTrackSkin.    
	@copy spark.components.supportClasses.TrackBase#track
	@see spark.skins.spark.HScrollBarTrackSkin -->
	<s:Button id="track" left="3" right="3" width="100%" 
			  focusEnabled="false" tabEnabled="false"
			  skinClass="style.skins.scrollbars.HScrollBarTrackSkin" />
	
	<!---  The default skin class is HScrollBarThumbSkin. 
	@copy spark.components.supportClasses.TrackBase#thumb
	@see spark.skins.spark.HScrollBarThumbSkin -->
	<s:Button id="thumb" left="3" right="3" width="100%"
			  focusEnabled="false" visible.inactive="false" tabEnabled="false"
			  skinClass="style.skins.scrollbars.HScrollBarThumbSkin" />
	

</s:Skin>
